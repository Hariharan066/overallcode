package org.String;
	
public class SwapString {
	public static void main(String[] args) {
		String s1="good";
		String s2="morning";
		s1=s1+s2;//goodmorning
        s1=s1.substring(s1.length()-s2.length());//
        s2=s1.substring(0,s2.length());
		System.out.println(s1);
		System.out.println(s2);
	}

}
